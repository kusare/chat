{
  "name": "chat-1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "format-fix": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "lint:style": "stylelint '**/*.{css,scss,sass}'",
    "lint:style:fix": "stylelint --fix '**/*.{css,scss,sass}'",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@codemirror/closebrackets": "^0.19.2",
    "@codemirror/commands": "^0.20.0",
    "@codemirror/gutter": "^0.19.9",
    "@codemirror/state": "^0.20.0",
    "@codemirror/view": "^0.20.5",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@inlet/react-pixi": "^6.8.0",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "css-generator": "^1.0.1",
    "csstype": "^3.1.0",
    "dayjs": "^1.11.2",
    "firebase": "^9.8.0",
    "next": "latest",
    "pixi.js": "^6.3.2",
    "react": "latest",
    "react-dom": "latest",
    "react-hammerjs": "^1.0.1",
    "recoil": "^0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "13.5.0",
    "@types/css-generator": "^1.0.2",
    "@types/hammerjs": "^2.0.41",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "18.0.3",
    "@types/react-hammerjs": "^1.0.4",
    "babel-loader": "^8.2.5",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "husky": "^7.0.4",
    "jest": "27.4.5",
    "lint-staged": "^12.4.1",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "storybook-addon-next": "^1.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "latest",
    "webpack": "^5.68.0"
  }
}
