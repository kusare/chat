    "@react-three/cannon": "^6.3.0",
    "@react-three/fiber": "^8.0.19",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/three": "^0.140.0",
    "cannon": "^0.6.2",
    "css-generator": "^1.0.1",
    "cssjson": "^2.1.3",
    "csstype": "^3.1.0",
    "dayjs": "^1.11.2",
    "firebase": "^9.8.0",
    "glslify-loader": "^2.0.0",
    "localforage": "^1.10.0",
    "modern-css-reset": "^1.4.0",
    "next": "^12.1.6",
    "next-pwa": "^5.5.2",
    "pixi.js": "^6.3.2",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-hammerjs": "^1.0.1",
    "react-redux": "^8.0.1",
    "react-timer-hook": "^3.0.5",
    "recoil": "^0.7.2",
    "recoil-persist": "^4.2.0",
    "redux-persist": "^6.0.0",
    "rss-parser": "^3.12.0",
    "soundfont-player": "^0.12.0",
    "three": "^0.140.2"
